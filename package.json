  "description": "Updates all dependencies to their latest versions (including major bumps)",
  "keywords": [
		"update-dependencies",
    "update-packages",
		"update-libs",
		"update-npm",
		"npm-update",
		"ts",
		"typescript"
	],
	"author": "JQ",
	"contributors": [],
	"repository": {
		"type": "git",
		"url": "https://github.com/yeikiu/bumplibs.git"
	},
	"bugs": {
		"url": "https://github.com/yeikiu/bumplibs/issues"
	},
  "bin": {
