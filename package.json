    "build": "rimraf dist && tsc -p ."
  },
  "devDependencies": {
		"@types/node": "^17.0.0",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
